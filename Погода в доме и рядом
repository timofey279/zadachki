import requests

lln = list(map(float, input().split()))
lat = lln[0]
lon = lln[1]

access_key = "a709f64e-90e9-4e3e-b31e-28dc96cdb513"

headers = {
    "X-Yandex-Weather-Key": access_key
}

param = {"humidity",
      "pressure",
      "temperature",
      "windSpeed",
      "windDirection"}

query1 = f"""{{
  weatherByPoint(request: {{lat: {lat}, lon: {lon}}}) {{
    now {{
      humidity
      pressure
      temperature
      windSpeed
      windDirection
    }}
  }}
}}"""

query2 = f"""{{
  weatherByPoint(request: {{lat: {lat}, lon: {lon + lln[2]}}}) {{
    now {{
      humidity
      pressure
      temperature
      windSpeed
      windDirection
    }}
  }}
}}"""


# –û—Ç–ø—Ä–∞–≤–ª—è–µ–º POST-–∑–∞–ø—Ä–æ—Å
# –í–∞–∂–Ω–æ: GraphQL –ø–æ—á—Ç–∏ –≤—Å–µ–≥–¥–∞ –∂–∏–≤—ë—Ç –Ω–∞ POST-–∑–∞–ø—Ä–æ—Å–∞—Ö, –ø–æ—Ç–æ–º—É —á—Ç–æ –º—ã –ø–µ—Ä–µ–¥–∞—ë–º —Ç–µ–ª–æ
response1 = requests.post(
    'https://api.weather.yandex.ru/graphql/query',
    headers=headers,
    json={'query': query1}
)
response2 = requests.post(
    'https://api.weather.yandex.ru/graphql/query',
    headers=headers,
    json={'query': query2}
)


# –ü—Ä–æ–≤–µ—Ä—è–µ–º, –±—ã–ª –ª–∏ —Å–µ—Ä–≤–µ—Ä –∫ –Ω–∞–º –±–ª–∞–≥–æ—Å–∫–ª–æ–Ω–µ–Ω (–∫–æ–¥ 200 ‚Äî –≤—Å—ë –æ–∫)
if response1.status_code == 200 and response2.status_code == 200:
    data1 = response1.json()
    data2 = response2.json()
    # –†–∞—Å–∫–∞–ø—ã–≤–∞–µ–º JSON, –∫–∞–∫ –∞—Ä—Ö–µ–æ–ª–æ–≥–∏
    for i in param:
        temp1 = data1['data']['weatherByPoint']['now'][i]
        temp2 = data2['data']['weatherByPoint']['now'][i]
        print(f"{i} {temp1} {temp2}")

    # –î–ª—è –ª—é–±–æ–ø—ã—Ç–Ω—ã—Ö: –≤—ã–≤–µ–¥–µ–º –≤—Å—ë –º—è—Å–æ –æ—Ç–≤–µ—Ç–∞
    # print(json.dumps(data, indent=2, ensure_ascii=False))
else:
    print("üî• –•—å—é—Å—Ç–æ–Ω, —É –Ω–∞—Å –ø—Ä–æ–±–ª–µ–º–∞:", response.status_code)
    print(response.text)
